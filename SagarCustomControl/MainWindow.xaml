<Window x:Class="SagarCustomControl.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SagarCustomControl"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel local:Class1.MyProperty="1"  >
        <local:OnlyABCD local:Class1.MyProperty="1"  OnEnterPress="{Binding EnterPress}"></local:OnlyABCD>
        <local:OnlyABCD OnEnterPress="{Binding EnterPress}"></local:OnlyABCD>
        <local:OnlyABCD OnEnterPress="{Binding EnterPress}"></local:OnlyABCD>
    </StackPanel>
</Window>
